{
  "policies": {
    "BackgroundAppUpdate": true,
    "Cookies": {
      "Behavior": "reject-tracker-and-partition-foreign",
      "BehaviorPrivateBrowsing": "reject-tracker-and-partition-foreign"
    },
    "DisplayBookmarksToolbar": "newtab",
    "DontCheckDefaultBrowser": true,
    "EncryptedMediaExtensions": {
      "Enabled": false
    },
    "ExtensionSettings": {
      "*": {
        "installation_mode": "allowed"
      },
      "feed-preview@code.guido-berhoerster.org": {
        "install_url": "https://addons.mozilla.org/firefox/downloads/latest/feed-preview/latest.xpi",
        "installation_mode": "normal_installed",
        "updates_disabled": true
      }
    },
    "ExtensionUpdate": false,
    "FirefoxHome": {
      "Search": false,
      "SponsoredPocket": false,
      "SponsoredTopSites": false
    },
    "Permissions": {
      "VirtualReality": {
        "BlockNewRequests": true
      }
    },
    "Preferences": {
      "browser.compactmode.show": {
        "Status": "locked",
        "Type": "boolean",
        "Value": true
      },
      "browser.contentblocking.category": {
        "Status": "default",
        "Type": "string",
        "Value": "strict"
      },
      "browser.menu.showViewImageInfo": {
        "Status": "default",
        "Type": "boolean",
        "Value": true
      },
      "browser.search.context.loadInBackground": {
        "Status": "default",
        "Type": "boolean",
        "Value": true
      },
      "browser.startup.page": {
        "Status": "default",
        "Type": "number",
        "Value": 3
      },
      "browser.tabs.insertAfterCurrent": {
        "Status": "default",
        "Type": "boolean",
        "Value": true
      },
      "browser.uidensity": {
        "Status": "user",
        "Type": "number",
        "Value": 1
      },
      "browser.urlbar.suggest.recentsearches": {
        "Status": "default",
        "Type": "boolean",
        "Value": false
      },
      "browser.urlbar.suggest.searches": {
        "Status": "default",
        "Type": "boolean",
        "Value": false
      },
      "browser.urlbar.suggest.trending": {
        "Status": "default",
        "Type": "boolean",
        "Value": false
      },
      "dom.private-attribution.submission.enabled": {
        "Status": "default",
        "Type": "boolean",
        "Value": false
      },
      "extensions.pocket.enabled": {
        "Status": "user",
        "Type": "boolean",
        "Value": false
      },
      "general.smoothScroll": {
        "Status": "user",
        "Type": "boolean",
        "Value": false
      },
      "privacy.globalprivacycontrol.enabled": {
        "Status": "default",
        "Type": "boolean",
        "Value": true
      },
      "privacy.userContext.enabled": {
        "Status": "default",
        "Type": "boolean",
        "Value": true
      },
      "widget.gtk.overlay-scrollbars.enabled": {
        "Status": "user",
        "Type": "boolean",
        "Value": false
      }
    },
    "SearchBar": "separate"
  }
}